<div class="col-lg-12 col-md-12 col-sm-12" style="height: 500px;">@*//googlemap*@
    <div id="map" style="height: 100%;"></div>
    <script>
            var map;
            function initMap() {
                var myLatLng = { lat: -33.892199, lng: 151.1772834 };

                map = new google.maps.Map(document.getElementById('map'), {
                    center: myLatLng,
                    zoom: 15
                });

                var marker = new google.maps.Marker({
                    position: myLatLng,
                    map: map,
                    title: 'Hello World!'
                });

                marker.setMap(map);
            }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBu2Gv_7D6IFK2wqXie76sZFCn-h5uzyv8&callback=initMap"
            async defer></script>
</div>

