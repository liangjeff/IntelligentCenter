@{
    ViewBag.Title = "Carparkinfo";
}
@section Scripts{
    @Scripts.Render("~/Angular/Facility/Carpark_parkingController.js")
}
<script>
    angular.module('Spartan', ["kendo.directives"]);
    angular.element(document).ready(function () {
        angular.bootstrap(document, ['Spartan']);
    });

    angular.module("Spartan").value("currentUserId", @Html.Raw(Json.Encode(ViewBag.CurrentUser.Id)));
</script>

<div ng-controller="carpark_parkingcontroller as ctrl">
    <h3>View parking spaces information</h3>
    <table class="table table-bordered">
        <thead>
            <tr style="height: 10px">
                
                <th class="text-center">Location Code</th>
                <th class="text-center">Latitude</th>
                <th class="text-center">Longtitude</th>
                <th class="text-center">Type Name</th>
                <th class="text-center">ParkingTime</th>
            </tr>
        </thead>
        <tbody>
        <tr ng-repeat="item in ctrl.parkinginfodatasource">
                <td><h3><span class="text-success">{{item.LocationCode}}</span></h3></td>
                <td><h3><span class="text-success">{{item.Latitude}}</span></h3></td>
                <td><h3><span class="text-success">{{item.Longtitude}}</span></h3></td>
                <td><h3><span class="text-success">{{item.TypeName}}</span></h3></td>
                <td><h3><span class="text-success">{{item.ParkingTime}}</span></h3></td>
              
            </tr>
          
         

    </table>
 
</div>
